{
  "name": "americons",
  "version": "1.0.0",
  "description": "SVGs and Icon Font for the geographic boundaries of the United States",
  "main": "index.js",
  "scripts": {
    "jekyll": "jekyll build --incremental --source www --destination site",
    "prestart": "npm-run-all templates fonts:sass copy2www",
    "precopy2www": "rimraf www/americons",
    "copy2www": "node tools/copy2www.js",
    "lint:sass": "sass-lint -c .sass-lint.yml -v",
    "presass": "npm-run-all fonts:sass lint:sass",
    "sass": "node-sass --source-map true --include-path www/sass --include-path node_modules www/sass/main.scss www/styles/bundle.css",
    "postsass": "postcss -c postcss.www.config.json",
    "start": "npm-run-all --parallel server:dev watch",
    "server:dev": "babel-node tools/runBrowserSync.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "fonts": "npm-run-all fonts:fonts fonts:version templates",
    "prefonts:fonts": "rimraf fonts",
    "fonts:fonts": "gulp fonts",
    "fonts:version": "node tools/generateVersionTxt.js",
    "prefonts:sass": "npm-run-all lint:sass",
    "fonts:sass": "node-sass --source-map true --include-path sass sass/main.scss css/americons.css",
    "postfonts:sass": "postcss -c postcss.americons.config.json",
    "templates": "node tools/applyTemplates.js",
    "watch": "npm-run-all --parallel watch:jekyll watch:sass",
    "watch:jekyll": "nodemon -w www -e html,md,png,jpg,gif,svg,yml,xml,js,css --ignore www/sass --delay 1 -x \"npm run jekyll\"",
    "watch:sass": "nodemon -e scss -w www/sass -x \"npm run sass\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bowmanmc/americons.git"
  },
  "author": "Michael Bowman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bowmanmc/americons/issues"
  },
  "homepage": "https://github.com/bowmanmc/americons#readme",
  "dependencies": {
    "autoprefixer": "^6.6.0",
    "bluebird": "^3.4.6",
    "cssnano": "^3.9.1",
    "d3": "^4.4.0",
    "d3-geo": "^1.4.0",
    "d3-geo-projection": "^1.2.1",
    "d3-selection": "^1.0.3",
    "d3-selection-multi": "^1.0.0",
    "d3-shape": "^1.0.4",
    "ejs": "^2.5.5",
    "filesize": "^3.3.0",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-iconfont": "^8.0.1",
    "gulp-iconfont-css": "^2.1.0",
    "gulp-rename": "^1.2.2",
    "jsdom": "^9.8.3",
    "mkdirp": "^0.5.1",
    "moment": "^2.17.1",
    "node-sass": "^4.1.1",
    "normalize.css": "^5.0.0",
    "npm-run-all": "^3.1.2",
    "postcss-cli": "^2.6.0",
    "rimraf": "^2.5.4",
    "sass-lint": "^1.10.2",
    "shapefile": "^0.5.9",
    "svgo": "^0.7.1",
    "sync-exec": "^0.6.2",
    "unzip": "^0.1.11"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "browser-sync": "^2.18.5",
    "copy-dir": "^0.3.0",
    "nodemon": "^1.11.0",
    "webpack": "^1.14.0"
  }
}
