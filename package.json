{
  "name": "americons",
  "version": "1.0.0",
  "description": "SVGs and Icon Font for the geographic boundaries of the United States",
  "main": "index.js",
  "scripts": {
    "json": "node tools/convertToJson.js",
    "start": "npm-run-all tiger unzip json",
    "svg": "node tools/convertToSvg.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean:json": "rimraf json",
    "clean:shp": "rimraf shp",
    "clean:svg": "rimraf svg",
    "clean": "npm-run-all clean:json clean:shp clean:svg",
    "optimize": "node tools/optimizeSvg.js",
    "preunzip": "npm run clean:shp",
    "unzip": "node tools/unzipTiger.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bowmanmc/americons.git"
  },
  "author": "Michael Bowman",
  "license": "MIT",
